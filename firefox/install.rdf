<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>googlesharing@extension.thoughtcrime.org</em:id>
    <em:version>0.21</em:version>
    <em:type>2</em:type>
    <em:optionsURL>chrome://googlesharing/content/options.xul</em:optionsURL>
    <em:iconURL>chrome://googlesharing/content/images/googlesharing.png</em:iconURL>
    <em:unpack>true</em:unpack>
   
    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion>5.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
   
    <!-- Front End MetaData -->
    <em:name>GoogleSharing</em:name>
    <em:description>Google Sharing</em:description>
    <em:creator>Moxie Marlinspike</em:creator>
    <em:homepageURL>http://www.googlesharing.net/</em:homepageURL>
  </Description>      
</RDF>
